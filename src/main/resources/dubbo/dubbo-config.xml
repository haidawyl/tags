<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://code.alibabatech.com/schema/dubbo
       http://code.alibabatech.com/schema/dubbo/dubbo.xsd
       http://www.springframework.org/schema/context
       http://www.springframework.org/schema/context/spring-context.xsd">


    <dubbo:annotation package="com.atme8.ddmp"></dubbo:annotation>
    <dubbo:application name="web-app" owner="atme" organization="atme" />
    <import resource="dubbo-${spring.profiles.active}.xml"/>

    <dubbo:reference id="insuserService" interface="com.atme8.project.insuser.service.InsuserService" />
</beans>